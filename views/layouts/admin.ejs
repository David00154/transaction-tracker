        <script src="https://demo.web3canvas.com/themeforest/startly/logistics/js/script.js" defer></script>

        <script>
            $(document).ready(async function () {
                $('.select-tracking-number').select2({
                    placeholder: "Please select tracking number - Identity",
                    // allowClear: true,
                })
                $('.tracking-status').select2({
                    placeholder: "Please select tracking status",
                    // allowClear: true,
                })
                $.getJSON("/api/get-tracking-numbers", (data) => {

                    $(".select-tracking-number").append("<option></option>")
                    data.map((value, i) => {
                        $(".select-tracking-number").append(new Option(`${value.tracking_number} - ${value.name}`, value.tracking_number, false, false)).trigger("change")
                    });
                })
                let status_point = ["Pending", "InProgress", "Approved"]
                $(".tracking-status").append("<option></option>")
                status_point.map((data) => {
                    $(".tracking-status").append(new Option(data, data, false, false)).trigger("change")
                })
                let submitBtns = document.querySelectorAll("#submit-btn")
                submitBtns.forEach((element) => {
                    element.addEventListener("click", () => {
                        element.textContent = 'Processing...'
                        element.ariaDisabled = true
                    })
                })
            })
        </script>
